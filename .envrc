export GOOGLETEST_MOCK_INCLUDE=$(dirname $(brew --prefix)/Cellar/googletest/*/include/.)
export GOOGLETEST_TEST_INCLUDE=$(dirname $(brew --prefix)/Cellar/googletest/*/include/.)
export GOOGLETEST_LIB=$(dirname $(brew --prefix)/Cellar/googletest/*/lib/.)

# Load Wi-Fi credentials from 1Password, if available.
if command -v op >/dev/null 2>&1; then
  export WIFI_SSID=${WIFI_SSID:-"$(op read 'op://Private/Home WiFi/network name' 2>/dev/null || true)"}
  export WIFI_PASSWORD=${WIFI_PASSWORD:-"$(op read 'op://Private/Home WiFi/wireless network password' 2>/dev/null || true)"}
fi
